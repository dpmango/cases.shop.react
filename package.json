{
  "name": "fortnite-frontend",
  "private": true,
  "version": "1.2.0",
  "homepage": "./",
  "engines": {
    "node": ">=16.15.0"
  },
  "type": "module",
  "scripts": {
    "dev": "npm run server",
    "build": "vite build --base=/",
    "serve": "vite preview",
    "server": "ts-node-esm ./server/index.ts",
    "server:prod": "cross-env NODE_ENV=production ts-node-esm ./server/index.ts",
    "lint:js:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:style:fix": "stylelint \"**/*.{css,scss}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:format && yarn lint:js:fix && yarn lint:style:fix"
  },
  "pre-commit": "lint",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.9",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "ofetch": "^1.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-photo-view": "^1.2.3",
    "react-redux": "^8.1.1",
    "react-router": "^6.14.1",
    "react-router-dom": "^6.14.1",
    "react-telegram-auth": "^1.0.4",
    "react-toastify": "^9.1.3",
    "swiper": "^9.2.3",
    "vite-plugin-ssr": "^0.4.133"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/js-cookie": "^3.0.3",
    "@types/react-redux": "^7.1.25",
    "@types/node": "^20.4.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "express": "^4.18.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "stylelint": "^15.10.1",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-order": "^6.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.6",
    "vite": "^4.4.2",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^4.0.0"
  }
}
