{
  "name": "fortnite-frontend",
  "private": true,
  "version": "1.1.0",
  "homepage": "./",
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build --base=/",
    "serve": "vite preview",
    "lint:js:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint:style": "stylelint \"**/*.{css,scss}\" --ignore-path .gitignore",
    "lint:style:fix": "stylelint \"**/*.{css,scss}\" --fix --ignore-path .gitignore",
    "lint": "yarn lint:format && yarn lint:js:fix && yarn lint:style:fix"
  },
  "pre-commit": "lint",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "bootstrap": "^5.2.3",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.7",
    "js-cookie": "^3.0.1",
    "nanoid": "^4.0.2",
    "ofetch": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.6",
    "react-photo-view": "^1.2.3",
    "react-redux": "^8.0.5",
    "react-router": "^6.10.0",
    "react-router-dom": "^6.10.0",
    "react-telegram-auth": "^1.0.4",
    "remark-gfm": "^3.0.1",
    "swiper": "^9.2.0",
    "vite-plugin-ssr": "^0.4.111"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "stylelint": "^15.3.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.3",
    "unplugin-auto-import": "^0.15.2",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-mkcert": "^1.14.0",
    "vite-plugin-singlefile": "^0.13.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-svg-loader": "^4.0.0"
  }
